    </para>
    <para id="para20">
      Then
      <m:math display="block">
	<m:apply>
	  <m:eq/>
	  <m:ci>
	    <m:msubsup>
	      <m:mi>ùí±</m:mi>
	      <m:mi>ii</m:mi>
	      <m:mi>‚àû</m:mi>
	    </m:msubsup>
	  </m:ci>
	  <m:apply>
	    <m:times/>
	    <m:ci>Œº</m:ci>
	    <m:apply>
	      <m:plus/>
	      <m:apply>
		<m:times/>
		<m:ci>
		  <m:msubsup>
		    <m:mi>ùí±</m:mi>
		    <m:mi>ii</m:mi>
		    <m:mi>‚àû</m:mi>
		  </m:msubsup>
		</m:ci>
		<m:apply>
		  <m:sum/>
		  <m:bvar>
		    <m:ci>j</m:ci>
		  </m:bvar>
		  <m:ci>
		    <m:msub>
		      <m:mi>Œª</m:mi>
		      <m:mi>j</m:mi>
		    </m:msub>
		  </m:ci>
		</m:apply>
	      </m:apply>
	      <m:apply>
		<m:power/>
		<m:ci>
		  <m:msub>
		    <m:mi>Œµ</m:mi>
		    <m:mi>min</m:mi>
		  </m:msub>
		</m:ci>
		<m:cn>2</m:cn>
	      </m:apply>
	    </m:apply>
	  </m:apply>
	</m:apply>
      </m:math>
      <m:math display="block">
